{
  "name": "esnow",
  "version": "1.0.0",
  "description": "Write ESXXXX JavaScript in a common way with Browserify and Babelify including extracted source maps and Uglifyify for production builds.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "test-compile": "esnow -e test/demo/main.js -o test/demo/out",
    "clean": "trash test/demo/out"
  },
  "keywords": [
    "babel",
    "browserify",
    "sourcemaps",
    "uglify"
  ],
  "author": "Kahlil Lechelt",
  "license": "MIT",
  "bin": {
    "esnow": "bin/cli.js"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "chalk": "^1.1.1",
    "exorcist": "^0.4.0",
    "minimist": "^1.1.3",
    "mkdirp": "^0.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.3.1"
  },
  "devDependencies": {
    "serve": "^1.4.0",
    "tape": "^4.2.0"
  }
}
